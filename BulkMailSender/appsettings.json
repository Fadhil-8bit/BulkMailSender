{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileUpload": {
    "MaxFileSizeMB": 500,
    "AllowedExtensions": [ ".zip" ],
    "UploadTimeoutMinutes": 10
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 524288000,
      "KeepAliveTimeout": "00:10:00",
      "RequestHeadersTimeout": "00:10:00"
    }
  }
}
